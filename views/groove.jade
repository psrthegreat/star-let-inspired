extends common
block extralinks
	link(rel='stylesheet', href='stylesheets/groove.css')

block content
	section.main
		.container
			.row#main-row
				#right.col-md-6.col-md-push-6
					h2 Playing Right Now...
					#player
					h2 Up Next...
				#left.col-md-6.col-md-pull-6
					form#hi
						textarea#imp
							|Hey


block extrascripts
	script(src="http://mobwrite3.appspot.com/static/compressed_form.js")
	script.
		mobwrite.syncGateway = 'http://mobwrite3.appspot.com/scripts/q.py';
		$(document).ready(function(){
			mobwrite.idPrefix = 'woah';
			mobwrite.nullifyAll = false;
			mobwrite.timeoutInterval = 3000000;
			mobwrite.share('imp');
		});
	script(src="http://www.youtube.com/player_api")
	script(src='javascripts/groove.js')